{
  "env": {
      "es6": true,
      "node": true
    },
    "extends": [
      "airbnb-base"
    ],
    "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 9,
        "sourceType": "module"
    },
    "rules": {
        "no-console": 1,
        "max-len": ["error", { "code": 100, "ignoreUrls": true }],
        "no-var": "error",
        "linebreak-style": 0,
        "import/no-extraneous-dependencies": ["error", { "devDependencies": true }]
    }
}